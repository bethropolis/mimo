function fibonacci(n)
    if <= n 1
        return n
    endif
    set a 0
    set b 1
    set i 2
    while <= i n
        call add(a, b) -> temp
        set a b
        set b temp
        set i + i 1
    endwhile
    return b
endfunction

function add(x, y)
    return + x y
endfunction

set number 10
call fibonacci(number) -> fib
show fib