{
  "comments": {
    "lineComment": "#"
  },
  "brackets": [
    [
      "(",
      ")"
    ],
    [
      "[",
      "]"
    ],
    [
      "{",
      "}"
    ]
  ],
  "autoClosingPairs": [
    [
      "(",
      ")"
    ],
    [
      "[",
      "]"
    ],
    [
      "{",
      "}"
    ],
    [
      "\"",
      "\""
    ]
  ],
  "surroundingPairs": [
    [
      "(",
      ")"
    ],
    [
      "[",
      "]"
    ],
    [
      "{",
      "}"
    ],
    [
      "\"",
      "\""
    ]
  ],
  "folding": {
    "markers": {
      "start": "^\\s*(?:function|fn|if|while|for|try|match)\\b",
      "end": "^\\s*end\\b"
    }
  },
  "indentationRules": {
    "increaseIndentPattern": "^\\s*(?:function|fn|if|else|while|for|try|catch|match|case|default)\\b.*$",
    "decreaseIndentPattern": "^\\s*(?:end|else|catch|case|default)\\b.*$"
  }
}